// Vars BEGIN
	
	// colors
	clr-prim = #EC4A5B
	clr-prim--drk = #CC3D4C
	
	clr-vk = #45668E
	clr-vk--drk = #2E4E75
	clr-inst = #e01b65
	clr-inst-drk = #C4275D
	clr-tg = #0088CC
	clr-tg-drk = #0077B2

	clr-title = #232323
	clr-txt = rgba(0,0,0,0.8)
	clr-txt--over = #fff
	
	clr-srfc = #ffffff
	clr-srfc--drk = #F5F5F5
	
	// sizes
	sz-regular--dsktp = 18px
	sz-title-dsktp = 50px
	sz-ad-title--dsktp = 36px
	sz1 = 26px
	sz2 = 62px
	sz2x = 124px
	sz22 = 68px
	
// Vars END


html
body
p
h1
h2
h3
h4
li
ul
	margin 0
	padding 0

html
	overflow-x hidden
	
body 
	overflow-x hidden
	font-family 'Montserrat', sans-serif

.container
	max-width 1260px
	margin 0 auto

body
	background-color clr-srfc--drk	

h1
h2
h3
h4
	color clr-title	
p
	font-size sz-regular--dsktp
	line-height 2
	color clr-txt

li
	list-style-type none

a
	text-decoration none
button
	font-size sz-regular--dsktp
	border none
	border-radius 12px

// hero

header
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	flex-direction column
	padding 16px 26px

.contact-btn
	background transparent
	border 3px solid white
	color white
	font-size 18px
	font-weight 600
	border-radius 12px
	padding 12px 26px
	margin-bottom 15px
	align-self flex-end
	&:hover
		background-color clr-prim
		border-color clr-prim
		transition all 1s

.header-tel
	color white
	font-size 18px
	font-weight 600
	align-self flex-end
	&:hover
		color clr-prim
		transition all 1s

.hero
	content ''
	box-sizing border-box
	height 100vh
	width 100vw
	background url("images/hero.jpg") center center
	background-repeat no-repeat
	background-size cover
	
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content center
	
	padding-bottom sz2x
	margin-bottom sz2x
	
	&-content
		margin 0 auto
		background transparent
		position absolute
		top 0
		left 0
		right 0

		
.cta-btn
	// width 300px
	font-size 18px
	font-weight 600
	background-color transparent
	color clr-txt--over
	border 3px white solid
	border-radius 12px
	padding 12px 26px
	// margin-left auto
	align-self flex-end
	
	&:hover
		background-color clr-prim
		border-color clr-prim
		transition all 1s
	
	

// hero




// Features END


.features
	margin-bottom sz2x
	
	&-shield
		align-self center

.features-list
	width 100%
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content space-between
	

.features-list__item
	position relative
	align-self flex-start
	max-width 357px
	padding 48px 68px
	
	background-color clr-srfc
	
	
	text-align center
	
	&--1
	&--2
	&--3
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex	
		flex-direction column
		justify-content center

	&--1
		&:before
			content ""
			position absolute
			display block
			width 124px
			height 91px
			// background-image "url(images/sub.png)"
			right 0
			left 0
			top -90px
	&--2
		&:before
			content ""
			position absolute
			display block
			width 124px
			height 91px
			// background-image "url(images/money.png)"
			right 0
			left 0
			top -90px
	&--3
		&:before
			content ""
			position absolute
			display block
			width 124px
			height 91px
			// background-image "url(images/lide.png)"
			right 0
			left 0
			top -90px

.features__title
	font-size 28px
	margin-bottom 16px

// Features BEGIN 


