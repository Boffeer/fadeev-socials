	font-size sz-title-dsktp
	margin-bottom sz2
h3
	font-size sz-ad-title--dsktp
	margin-bottom sz2

.belarus-wrap
	width 100%
	background-color clr-srfc
	box-shadow 0px 0px 0.562399px rgba(0, 0, 0, 0.04), 0px 2.24959px 4.49919px rgba(0, 0, 0, 0.04), 0px 8.99838px 13.4976px rgba(0, 0, 0, 0.04), 0px 13.4976px 17.9968px rgba(0, 0, 0, 0.04)
	border-radius 10px
	padding-bottom sz22
	margin 0 atuo sz2x auto

.belarus-hero
	width 100%
	height 315px
	border-radius 10px 10px 0px 0px
	// object-fit cover
	


.community-container
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	flex-wrap wrap
	justify-content space-between
	
	padding sz22
	padding-bottom 0
	
	overflow-y hidden

.community-item
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	flex-direction column
	
	width 234px
	box-shadow 0px 0px 0.562399px rgba(0, 0, 0, 0.04), 0px 2.24959px 4.49919px rgba(0, 0, 0, 0.04), 0px 8.99838px 13.4976px rgba(0, 0, 0, 0.04), 0px 13.4976px 17.9968px rgba(0, 0, 0, 0.04)
	border-radius 10px
	padding 14px 0
	margin-bottom sz22
	text-align center
	
	&:hover
		transition all 0.5s
		cursor pointer
		background-color clr-prim
		
		h4
		p
		button 
			color clr-txt--over
		.community-item__more
			img:nth-child(2)
				display none
				transition all 0.5s
			img:nth-child(1)
				display inline-block	
				// margin-left 10px
				opacity 1
				transition all 0.5s
			
			
		
			


.community-item__logo
	width 208px
	height 208px
	margin 0 auto 26px
	border-radius 200px
	

.community-item__name
	font-size 20px
	margin-bottom 5px
	

.community-subs
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content center


.community-item__subs
	margin-right 5px
	
img.arrow
	display none


.community-item__more
	position relative

	padding 16px 0 16px 30px
	background-color transparent
	color clr-prim
	cursor pointer
	img:nth-child(1)
			display none
			transition all 0.5s
			opacity 0
	&:hover
		img:nth-child(2)
			display none
			transition all 0.5s
		img:nth-child(1)
			display inline-block	
			margin-left 10px
			opacity 1
			transition all 0.5s
		
.belarus-form
	box-sizing border-box
	margin 0 68px 
	padding 68px 58px
	box-shadow 0px 0px 0.562399px rgba(0, 0, 0, 0.04), 0px 2.24959px 4.49919px rgba(0, 0, 0, 0.04), 0px 8.99838px 13.4976px rgba(0, 0, 0, 0.04), 0px 13.4976px 17.9968px rgba(0, 0, 0, 0.04)
	border-radius 10px
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	justify-content space-between

.form-title
